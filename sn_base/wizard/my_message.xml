<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>


	<record id="my_message_wizard" model="ir.ui.view">
		<field name="name">sn_base message wizard</field>
		<field name="model">sn_base.message_wizard</field>
		<field name="arch" type="xml">
			<form string="Message">
				<field name="name"/>

				<footer>
					<button string="OK" class="btn-primary" special="cancel" />
				</footer>

			</form>
		</field>
	</record>

</data>
</odoo>